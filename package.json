{
  "name": "react-native-boilerplate",
  "version": "2.3.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "start": "npx expo start -c",
    "start:dev": "npx expo start -c",
    "android": "DARK_MODE=media npx expo start --android",
    "android:dev": "npx expo start --android",
    "ios": "DARK_MODE=media npx expo start --ios",
    "ios:dev": "npx expo start --ios",
    "doctor:dev": "EXPO_NO_DOTENV=1 dotenv -e .env.development -- npx expo-doctor",
    "format": "prettier --config .prettierrc src/**/*.{ts,tsx} --write",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --verbose --passWithNoTests",
    "test:watch": "jest --verbose --passWithNoTests --watch",
    "prepare": "husky",
    "web": "DARK_MODE=media undefined"
  },
  "dependencies": {
